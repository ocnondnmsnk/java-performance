interface USB{
	public void open();
	public void close();
}

class BookPC{
	public static void main(String[] args) {
		useUSB(new UPan());
	}
	public static void useUSB(USB u)// 接口类型的引用,用于接收接口的子类对象
  {
		u.open();
		u.close();
	}
}

class UPan implements USB{
	public void open() {
		System.out.println("upan open");
	}
	public void close() {
		System.out.println("upan close");
	}
}
